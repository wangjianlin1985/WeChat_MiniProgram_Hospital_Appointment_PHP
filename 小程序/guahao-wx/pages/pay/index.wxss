.money{
  width: 100%;
  padding-top: 50px;
  background: #fff;
}
.money text{
  width: 100%;
  height: 50px;
  display: block;
  line-height: 30px;
  background: #fff;
  font-size: 20px;
  text-align: center;
  color: #999999;
}
.money view{
  line-height: 100px;
  background: #fff;
  font-size: 50px;
  text-align: center;
  color: #ffa14e;
  margin-bottom: 40px;
}
.main{
  margin-bottom: 40px;
}
.head-list{
  padding: 15px 20px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #ececec;
  color:#b2b2b2;
  
}

.input-set{
  width: 200px;
  text-align: right;
  color: #333;
}

