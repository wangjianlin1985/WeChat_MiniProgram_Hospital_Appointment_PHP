<view class="money">
  <text>订单金额</text>
  <view>￥{{order.totals}}</view>
</view>
<view class="main">
  <view class="head-list">
    <view>订单号</view>
    <view class="input-set">{{order.order_no}}</view>
  </view>
  <view class="head-list">
    <view>预约医生</view>
    <view class="input-set">{{order.doctor}}</view>
  </view>
  <view class="head-list">
    <view>预约日期</view>
    <view class="input-set">{{order.date}} </view>
  </view>
  <view class="head-list">
    <view>预约时间</view>
    <view class="input-set">{{order.time}} </view>
  </view>
  <view class="head-list">
    <view>就诊人</view>
    <view class="input-set">{{order.card}} </view>
  </view>
</view>
<button bindtap="payOrder" type="primary">订单支付</button>