.fphs {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  line-height: 200rpx;
  border: 1px solid coral;
  margin: 50rpx auto 20rpx auto;
  text-align: center;
  color: #666;
  font-size: 12px;
}

.fphs .circle {
  width: 180rpx;
  height: 180rpx;
  margin: 10rpx auto;
  border-radius: 50%;
  border: 1px dashed coral;
}

.fphs text {
  color: coral;
  font-size: 16px;
  margin-right: 5rpx;
}

